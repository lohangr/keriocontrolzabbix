                            <key>hrMemorySize.[{#SNMPVALUE}]</key>
                            <delay>84600s</delay>
                            <history>60d</history>
                            <trends>90d</trends>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>Memory</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>1024</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Buffering Memory Usage</name>
                            <type>SNMPV2</type>
                            <snmp_community>public</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.2021.4.14.{#SNMPINDEX}</snmp_oid>
                            <key>memBuffer.[{#SNMPVALUE}]</key>
                            <delay>30s</delay>
                            <history>60d</history>
                            <trends>90d</trends>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>Memory</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>1024</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Used memory (percent)</name>
                            <type>CALCULATED</type>
                            <key>memory.usage.percent.[{#SNMPVALUE}]</key>
                            <history>60d</history>
                            <trends>90d</trends>
                            <units>%</units>
                            <params>(100 * last(&quot;fgHaStatsMemUsage.[{#SNMPVALUE}]&quot;) / (last(&quot;hrMemorySize.[{#SNMPVALUE}]&quot;) * 1024))</params>
                            <applications>
                                <application>
                                    <name>Memory</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Share Memory Usage</name>
                            <type>SNMPV2</type>
                            <snmp_community>public</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.2021.4.13.{#SNMPINDEX}</snmp_oid>
                            <key>memShared.[{#SNMPVALUE}]</key>
                            <delay>60s</delay>
                            <history>60d</history>
                            <trends>90d</trends>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>Memory</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>1024</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Free Total Memory</name>
                            <type>SNMPV2</type>
                            <snmp_community>public</snmp_community>
                            <snmp_oid>.1.3.6.1.4.1.2021.4.11.{#SNMPINDEX}</snmp_oid>
                            <key>memTotalFree.[{#SNMPVALUE}]</key>
                            <delay>60s</delay>
                            <history>60d</history>
                            <trends>90d</trends>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>Memory</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>1024</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>Memory usage {#SNMPVALUE}</name>
                            <graph_items>
                                <graph_item>
                                    <color>0040FF</color>
                                    <item>
