                <discovery_rule>
                    <name>LLD Discovery Firewall Description</name>
                    <type>SNMPV2</type>
                    <snmp_community>public</snmp_community>
                    <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.2.1.1.1]</snmp_oid>
                    <key>sysDescr.discovery</key>
                    <delay>84600s</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Number of services</name>
